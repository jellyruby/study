[사용자 정의 함수 생성]

CREATE OR REPLACE FUNCTION func(a NUMBER, b NUMBER) RETURN NUMBER IS var_sum NUMBER; BEGIN var_sum := a + b; RETURN var_sum; END;

[사용자 정의 함수 호출]

SELECT func(1,2) FROM DUAL;


[프로시저 생성 및 호출]

CREATE OR REPLACE PROCEDURE EX_PROC(
   EMP_NO IN int,
   
   SAL_FROM_DATE IN date
)
IS
SALARY int  := 123;

BEGIN

INSERT INTO SALARIES (EMP_NO, SALARY, SAL_FROM_DATE)
VALUES (EMP_NO, SALARY, SAL_FROM_DATE);
COMMIT;

END EX_PROC;

EXEC EX_PROC(500,'2017-07-01');
